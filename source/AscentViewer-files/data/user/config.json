{
    "windowProperties": {
        "width": 800,
        "height": 700,
        "x": 100,
        "y": 100,
        "bottomSplitterPanelH": 90
    },
    "theme": {
        "name": "AscentedNordDark",
        "style": "Fusion",
        "accentColorMain": "#777cc1",
        "accentColorDarker": "#525685",
        "accentColorLighter": "#a3a8e6",
        "accentColorIsALightColor": false
    },
    "localization": {
        "lang": "en-us"
    },
    "prompts": {
        "enableExitPrompt": true
    },
    "debug": {
        "enableDebugMenu": true,
        "logging": {
            "loggingLevel": "INFO"
        }
    },
    "temporary_files": {
        "logs": {
            "deleteLogsOnStartup": true
        }
    },
    "experimental": {
        "enableExperimentalUI": false
    },
    "hellMode": false
}
